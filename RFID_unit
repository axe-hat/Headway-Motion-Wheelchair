#include<SPI.h>
 #include<MFRC522.h>
 #include<LiquidCrystal_I2C.h>
 #definerstp9
 #definessp10
 #definemsgpn7
 MFRC522reada(ssp,rstp);
 Stringmastag="D3FD4C2E";
 Stringreadag="";
 LiquidCrystal_I2Clc(0x27,16,2);
 voidsetup(){
pinMode(msgpn,OUTPUT);
 Serial.begin(115200);
 SPI.begin();
 reada.PCD_Init();
 delay(4);
 lc.init();
 lc.backlight();
 lc.setCursor(0,0);
 lc.print("initialized");
 delay(3000);
 }
 voidloop(){
 lc.clear();
 while(gidID()){
 if(readag==mastag){
 lc.clear();
 lc.print("Userdetected");
 delay(2000);
 lc.setCursor(0,1);
 lc.print("accessgranted");
 digitalWrite(msgpn,HIGH);
 }
 else{
 lc.print("Accessdenied");
 delay(2000);
 lc.setCursor(0,1);
 lc.print("tryagain");
 }
 lc.setCursor(0,1);
 delay(2000);
 }
}
 boolgidID(){
 if(!reada.PICC_IsNewCardPresent()){returnfalse;}
 if(!reada.PICC_ReadCardSerial()){returnfalse;}
 readag="";
 for(uint8_ti=0;i<4;i++){
 readag.concat(String(reada.uid.uidByte[i],HEX));
 }
 readag.toUpperCase();
 reada.PICC_HaltA();
 returntrue;
 }
