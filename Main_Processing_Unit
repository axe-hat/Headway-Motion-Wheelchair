 #include<Adafruit_MPU6050.h>
 #include<Adafruit_Sensor.h>
 #include<Wire.h>
 #definercvpn5
 Adafruit_MPU6050head;
 voidsetup(void){
 Wire.begin();
 pinMode(LED_BUILTIN,OUTPUT);
 //MOTOR////////////////////////////////////
 //right
 pinMode(4,OUTPUT);
 pinMode(2,OUTPUT);
 pinMode(10,OUTPUT);
 //left
 pinMode(7,OUTPUT);
 pinMode(8,OUTPUT);
 pinMode(11,OUTPUT);
 //MOTOR///////////////////////////////////
 //GYRO////////////////////////////////////
 Serial.begin(115200);
 while(!Serial)
 delay(10);
 Serial.println("gyroonline");
 if(!head.begin()){
 Serial.println("gyro'snotworking");
 while(1){
 delay(10);
 }
 }
 Serial.println("gyro'sworkin'");
 head.setAccelerometerRange(MPU6050_RANGE_2_G);
 Serial.println("G'sset");
 Serial.println("");
 delay(100);
 //GYRO///////////////////////////////////
 }
//MOTOR//////////////////////////////////
 voidright(intdir,intrate){
 if(dir==1){
 digitalWrite(4,HIGH);
 digitalWrite(2,LOW);
 analogWrite(11,rate);
 }
 else{
 digitalWrite(4,LOW);
 digitalWrite(2,HIGH);
 analogWrite(11,rate);
 }
 }
 voidleft(intdir,intrate){
 if(dir==1){
 digitalWrite(8,LOW);
 digitalWrite(7,HIGH);
 analogWrite(10,rate);
 }
 else{
 digitalWrite(8,HIGH);
 digitalWrite(7,LOW);
 analogWrite(10,rate);
 }
 }
 voidstop(){
 right(0,0);
 left(0,0);
 }
 //MOTOR////////////////////////////////////
 voidloop(){
 /*
 Serial.print(a.acceleration.x);
 Serial.print(" ");
 Serial.print(a.acceleration.z);
 Serial.print(" ");
 Serial.print(a.acceleration.y);
 Serial.println(" ");
 delay(500);
 */
 while(digitalRead(rcvpn)){
 digitalWrite(LED_BUILTIN,HIGH);
 sensors_event_ta,g,temp;
 head.getEvent(&a,&g,&temp);
 if(a.acceleration.y<=6.00)
{
 if(a.acceleration.y>=-6.00){
 if(a.acceleration.x>=6.00){
 right(1,100);
 left(1,100);
 }
 else
 if(a.acceleration.x<=-6.00){
 right(0,100);
 left(0,100);
 }
 else{stop();}
 }
 else{
 right(0,100);
 left(1,100);
 }
 }
 else{
 right(1,100);
 left(0,100);
 }
 }
 }
